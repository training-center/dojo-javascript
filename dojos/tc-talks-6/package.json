{
  "name": "tc-talks-6",
  "version": "1.0.0",
  "description": "Projeto criado no Dojo realizado no TC Talks #6 ",
  "main": "index.js",
  "scripts": {
    "start": "node index.js --require babel-register",
    "prepush": "npm run test:coverage",
    "lint": "./node_modules/.bin/eslint *.js",
    "test": "./node_modules/.bin/mocha tests/**/*.test.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.test.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/training-center/dojos.git"
  },
  "keywords": [
    "js",
    "java",
    "script",
    "dojo",
    "tc",
    "talks",
    "training",
    "center"
  ],
  "author": "Training Center",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/training-center/dojos/issues"
  },
  "homepage": "https://github.com/training-center/dojos#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "mocha": "^5.0.1",
    "nyc": "^11.5.0"
  }
